# Production Environment Overrides
NODE_ENV=production
LOG_LEVEL=info

# Production-specific ORB parameters
TRADE_COOLDOWN_MINUTES=5
PERFORMANCE_LOGGING_ENABLED=true
ORDER_QTY=20

# Production API endpoints (when ready for live trading)
# ALPACA_API_BASE_URL=https://api.alpaca.markets

# Production risk management
MAX_POSITION_SIZE=50
